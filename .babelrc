{
  "env": {
    "commonjs": {
      "presets": [
        [
          "env",
          {
            "useBuiltIns": false
          }
        ],
        "babel-preset-es2015"
      ],
      "plugins": ["transform-object-rest-spread"]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "useBuiltIns": false,
            "modules": false
          }
        ]
      ],
      "plugins": ["transform-object-rest-spread"]
    }
  }
}
